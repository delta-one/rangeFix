}

CYGSEM_WALLCLOCK_MODE {
	default_value [CYGINT_WALLCLOCK_SET_GET_MODE_SUPPORTED > 0 ?  "set_get" : "init_get" ] replacing [CYGINT_WALLCLOCK_SET_GET_MODE_SUPPORTED ?  "set_get" : "init_get" ]
}
