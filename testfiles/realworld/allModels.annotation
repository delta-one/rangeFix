CYGNUM_HAL_ARM_LPC24XX_I2C2_CLK{
	calculated [CYGNUM_HAL_ARM_LPC24XX_CLOCK_SPEED / toInt(CYGNUM_HAL_ARM_LPC24XX_I2C2_CLK_DIV) ] replacing [CYGNUM_HAL_ARM_LPC24XX_CLOCK_SPEED / CYGNUM_HAL_ARM_LPC24XX_I2C2_CLK_DIV]
